#Creating subscale variables for critical action and getting Cronbach's Alpha
```{r}

ANES_GSS2$CA_con <- ifelse(ANES_GSS2$V202034 == 1 | ANES_GSS2$V202036 == 1 | ANES_GSS2$V202038 == 1 | ANES_GSS2$V202040 == 1, 1, 0)

ANES_GSS2$CA_boyc <- ifelse(ANES_GSS2$V202042 == 2| ANES_GSS2$V202042 == 3| ANES_GSS2$V202042 == 4 | ANES_GSS2$V202042 == 5, 1, 0)

ANES_GSS2$CA_don <- ifelse(ANES_GSS2$V202021 == 1, 1, 0)

ANES_GSS2$CA_don_o <- ifelse(ANES_GSS2$V202028 == 1, 1, 0)

ANES_GSS2$CA_pro <- ifelse(ANES_GSS2$V202025 == 1, 1, 0)

ANES_GSS2$CA_meetIP <- ifelse(ANES_GSS2$V202013 == 1, 1, 0)

ANES_GSS2$CA_meetON <- ifelse(ANES_GSS2$V202014 == 1, 1, 0)

ANES_GSS2$CA_pet <- ifelse(ANES_GSS2$V202026 == 1, 1, 0)

ANES_GSS2$CA_cam <- ifelse(ANES_GSS2$V202016 == 1, 1, 0)



alpha_result <- alpha(ANES_GSS2[, c("CA_con", "CA_pro", "CA_pet", "CA_meetIP", "CA_meetON", "CA_boyc", "CA_cam", "CA_don", "CA_don_o")])

print(alpha_result)

ANES_GSS2$CA_mean <- (ANES_GSS2$CA_con + ANES_GSS2$CA_pro + ANES_GSS2$CA_pet + ANES_GSS2$CA_meetIP + ANES_GSS2$CA_meetON + ANES_GSS2$CA_boyc + ANES_GSS2$CA_cam + ANES_GSS2$CA_don + ANES_GSS2$CA_don_o)/9
```

#Getting Cronbach's Alpha for CR and CA together
```{r}
alpha_result <- alpha(ANES_GSS2[, c("CA_con", "CA_pro", "CA_pet", "CA_meetIP", "CA_meetON", "CA_boyc", "CA_cam", "CA_don", "CA_don_o", "CR_slav", "CR_rpdif", "CR_ins", "CR_bus", "CR_opp", "CR_wrk", "CR_bld", "CR_vio", "CR_bi", "CR_hi", "CR_bwfed", "CR_bwpol", "CR_uu", "CR_pd")])

print(alpha_result)
```
#Creating subscale variables for Critical Reflection and getting Cronbach's Alpha
```{r}
ANES_GSS2$CR_DB <- ifelse(ANES_GSS2$V202527 == 1 | ANES_GSS2$V202527 == 2, 1, 0)

ANES_GSS2$CR_DH <- ifelse(ANES_GSS2$V202528 == 1 | ANES_GSS2$V202528 == 2, 1, 0)

ANES_GSS2$CR_DA <- ifelse(ANES_GSS2$V202529 == 1 | ANES_GSS2$V202529 == 2, 1, 0)

ANES_GSS2$CR_DM <- ifelse(ANES_GSS2$V202531 == 1 | ANES_GSS2$V202531 == 2, 1, 0)

ANES_GSS2$CR_DW <- ifelse(ANES_GSS2$V202534 == 1 | ANES_GSS2$V202534 == 2, 1, 0)

ANES_GSS2$CR_DGL <- ifelse(ANES_GSS2$V202533 == 1 | ANES_GSS2$V202533 == 2, 1, 0)

ANES_GSS2$CR_DT <- ifelse(ANES_GSS2$V202536 == 1 | ANES_GSS2$V202536 == 2, 1, 0)

ANES_GSS2$CR_RP <- ifelse(ANES_GSS2$V202305 == 4 | ANES_GSS2$V202305 == 5, 1, 0)

alpha_result <- alpha(ANES_GSS2[, c("CR_DB", "CR_DH", "CR_DA", "CR_DM", "CR_DW", "CR_DGL", "CR_DT", "CR_RP")])

print(alpha_result)

ANES_GSS2$CR_mean <- (ANES_GSS2$CR_DB + ANES_GSS2$CR_DH + ANES_GSS2$CR_DM + ANES_GSS2$CR_DA + ANES_GSS2$CR_DW + ANES_GSS2$CR_DGL + ANES_GSS2$CR_DT + ANES_GSS2$CR_RP)/8
```

#new CR measure
```{r}
#reverse coding for V202414
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202414 = replace(V202414, V202414 %in% c(1), 5))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202414 = replace(V202414, V202414 %in% c(2), 4))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202414 = replace(V202414, V202414 %in% c(4), 2))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202414 = replace(V202414, V202414 %in% c(5), 1))

#reverse coding for V202260
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202260 = replace(V202260, V202260 %in% c(1), 5))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202260 = replace(V202260, V202260 %in% c(2), 4))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202260 = replace(V202260, V202260 %in% c(4), 2))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202260 = replace(V202260, V202260 %in% c(5), 1))

#recoding scale from 7 to 5 for V202490x
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202490x = replace(V202490x, V202490x %in% c(1, 2), 5))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202490x = replace(V202490x, V202490x %in% c(3), 4))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202490x = replace(V202490x, V202490x %in% c(4), 3))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202490x = replace(V202490x, V202490x %in% c(5), 2))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202490x = replace(V202490x, V202490x %in% c(6, 7), 1))

#recoding scale from 7 to 5 for V202493x
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202493x = replace(V202493x, V202493x %in% c(1, 2), 5))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202493x = replace(V202493x, V202493x %in% c(3), 4))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202493x = replace(V202493x, V202493x %in% c(4), 3))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202493x = replace(V202493x, V202493x %in% c(5), 2))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202493x = replace(V202493x, V202493x %in% c(6, 7), 1))

#recoding scale from 7 to 5 in reverse for V202621
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202621 = replace(V202621, V202621 %in% c(1, 2), 5))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202621 = replace(V202621, V202621 %in% c(6, 7), 1))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202621 = replace(V202621, V202621 %in% c(3), 4))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202621 = replace(V202621, V202621 %in% c(4), 3))
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202621 = replace(V202621, V202621 %in% c(5), 2))

ANES_GSS2$CR_bi <- ANES_GSS2$V202495
ANES_GSS2$CR_hi <- ANES_GSS2$V202496
ANES_GSS2$CR_bwfed <- ANES_GSS2$V202490x
ANES_GSS2$CR_bwpol <- ANES_GSS2$V202493x
ANES_GSS2$CR_uu <- ANES_GSS2$V202621
ANES_GSS2$CR_pd <- ANES_GSS2$V202414
ANES_GSS2$CR_cc <- ANES_GSS2$V202333
ANES_GSS2$CR_cca <- ANES_GSS2$V202334
ANES_GSS2$CR_lies <- ANES_GSS2$V202312
ANES_GSS2$CR_wom <- ANES_GSS2$V202291
ANES_GSS2$CR_wor <- ANES_GSS2$V202261
ANES_GSS2$CR_equal <- ANES_GSS2$V202260
ANES_GSS2 <- ANES_GSS2 %>%
  rename(CR_slav = V202301, CR_rpdif = V202305, CR_ins = V202304, CR_bus = V202311, CR_opp = V202317, CR_wrk = V202303, CR_bld = V202302, CR_vio = V202579)

alpha_result <- alpha(ANES_GSS2[, c("CR_slav", "CR_rpdif", "CR_ins", "CR_bus", "CR_opp", "CR_wrk", "CR_vio", "CR_bwpol", "CR_pd", "CR_cc", "CR_wom", "CR_wor", "CR_equal")])

print(alpha_result)

ANES_GSS2$CR2_mean <- (ANES_GSS2$CR_slav + ANES_GSS2$CR_rpdif + ANES_GSS2$CR_ins + ANES_GSS2$CR_bus + ANES_GSS2$CR_opp + ANES_GSS2$CR_wrk + ANES_GSS2$CR_bld + ANES_GSS2$CR_vio + ANES_GSS2$CR_bwpol + ANES_GSS2$CR_pd + ANES_GSS2$CR_cc + ANES_GSS2$CR_wom + ANES_GSS2$CR_wor + ANES_GSS2$CR_equal) /14
```


#work on kids and ca
```{r}

ANESGSS_merged$kids_b <- ifelse(ANESGSS_merged$KIDSSOL_1B == 6, 1, 0)

model <- lm(CA_mean ~ kids_b, data = ANESGSS_merged)

summary(model)

```
#linear model on kids' sol and CA
```{r}
data_subset <- subset(ANESGSS_merged, KIDSSOL_1B != 6)

model <- lm(CA_mean ~ KIDSSOL_1B, data = data_subset)

summary(model)

```


