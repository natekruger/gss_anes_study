#view item v202023
```{r}

#view the contents of item v202023

table(ANES_GSS2$V202023)

#check to see the variable type as recorded in R
class(ANES_GSS2$V202023)


#make this variable numeric
ANES_GSS2$V202023 <- as.numeric(ANES_GSS2$V202023)


# Recode -1 and -9 to NA in the V202023 variable
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202023 = replace(V202023, V202023 %in% c(-1, -9), NA))


#view the contents of item v202023 after recoding

