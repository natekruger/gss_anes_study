#Creating subscale variables for Critical Motivation and getting Cronbach's Alpha
```{r}
ANES_GSS2$CM_int <- ifelse(ANES_GSS2$V202406 == 1 | ANES_GSS2$V202406 == 2, 1, 0)

ANES_GSS2$CM_fol <- ifelse(ANES_GSS2$V202407 == 1 | ANES_GSS2$V202528 == 2, 1, 0)

ANES_GSS2$CM_soceq <- ifelse(ANES_GSS2$V202260 == 1 | ANES_GSS2$V202260 == 2, 1, 0)

ANES_GSS2$CM_II <- ifelse(ANES_GSS2$V202257 == 1, 1, 0)

ANES_GSS2$CM_PP <- ifelse(ANES_GSS2$V202414 == 1 | ANES_GSS2$V202414 == 2, 1, 0)


alpha_result <- alpha(ANES_GSS2[, c("CM_int", "CM_fol", "CM_soceq", "CM_II", "CM_PP")])

print(alpha_result)
```

