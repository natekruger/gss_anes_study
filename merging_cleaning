#rename YEAR to YEARID for merging
```{r}
GSS2018 <- GSS2018 %>%
  rename(YEARID = YEAR)
```

#Join data frames
```{r}

#check the class of the ID variable, they both need to be numeric
class(ANES_GSS2$YEARID)

#make the yearID variable numeric
ANES_GSS2$YEARID <- as.numeric(ANES_GSS2$YEARID)


#check class of the GSS2018 ID variable
class(GSS2018$YEARID)

#make the yearID variable in GSS2018 numeric
GSS2018$YEARID <- as.numeric(GSS2018$YEARID)



#check documentation on inner_join, this unlike full_join should only keep the participants whose ID is in both datsets. 
?inner_join 

ANESGSS_merged <- inner_join(ANES_GSS2, GSS2018, by = "YEARID")

#ANESGSS_merged2 <- full_join(ANES_GSS2, GSS2018, by = "YEARID")

#ANESGSS_merged[ANESGSS_merged=="-1"] <- NA
#ANESGSS_merged[ANESGSS_merged=="-1"] <- NA
```




#Save the Wide format Combined data
```{r}
#Save merged dataframe as a .csv
write.csv(ANESGSS_merged, "ANESGSS_merged2.csv")
```

