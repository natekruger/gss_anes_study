```{r}

#recoding negative variables to NA for V202014
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202014 = replace(V202014, V202014 %in% c(-9), NA))

#recoding negative variables to NA for V202015
ANES_GSS2 <- ANES_GSS2 %>%
  mutate(V202015 = replace(V202015, V202015 %in% c(-9), NA))

